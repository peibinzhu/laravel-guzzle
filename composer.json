{
  "name": "peibin/laravel-guzzle",
  "description": "Peibin guzzle component for Laravel.",
  "license": "MIT",
  "keywords": [
    "php",
    "swoole",
    "laravel",
    "guzzle",
    "handler"
  ],
  "authors": [
    {
      "name": "Peibin Zhu",
      "email": "zhupeibin1207@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "guzzlehttp/guzzle": "^6.3|^7.0",
    "peibin/laravel-coroutine": "^1.0",
    "peibin/laravel-engine": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "PeibinLaravel\\Guzzle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PeibinLaravelTest\\Guzzle\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
